function getLatLongWareHouse(){
  var encontre = false;
  var latlng;
  msg("***getLatLongHareHouse***");
  for(i=0; ((i<vectorOrigenes.length) && (!encontre)); i++){
    if (vectorOrigenes[i].OrigenDefault == "S"){
      o = vectorOrigenes[i];
