function inconoEstadoSalavadoPoly(id){
  msg("***inconoEstadoSalavadoPoly***");
  //Get estado poly
  var poly= getPolyById(id);
  var icono = "";
  if (poly==undefined){
    icono = "editar.svg";
  }else{
    switch(poly.estadosalvado){
       case "0":
            icono = "editar.svg";
            break;
       case "1":
            icono = "guardar.svg";
            break;
       default:
            msg('default');break;
    }
  }
  return PATHIMAGES + "/" + icono;

}

